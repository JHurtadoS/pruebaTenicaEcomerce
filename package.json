  "private": true,
  "scripts": {
    "lint": "next lint",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "vitest --run",
    "test:unit": "vitest --run ",
    "test:integration": "vitest --run src/test/integration/**/*.test.{ts,tsx}",
