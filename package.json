    "docker:test:integration": "docker-compose run --rm app npm run test:integration",
    "docker:test:e2e": "docker-compose run --rm app npm run test:e2e",
    "docker:test:all": "npm run docker:test:unit && npm run docker:test:integration && npm run docker:test:e2e"
